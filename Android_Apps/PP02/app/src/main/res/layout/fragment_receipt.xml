<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent"
                                                   tools:context=".ReceiptFragment" android:id="@+id/frameLayout3">
    <TextView
            android:text="@string/totalCost"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/totalCost" android:layout_marginTop="16dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="16dp"/>
    <TextView
            android:text="@string/walletTotal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/moneyLeft" android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/totalCost" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="16dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>